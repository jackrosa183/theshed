<% content_for :title, "Tunes" %>

<div class="flex flex-col justify-center mx-auto gap-2">
  <h1 class="text-8xl">Tunes</h1>

  <div id="tunes">
    <% @tunes.each do |tune| %>
      <%= render tune %>
      <p>
        <%= link_to "Show this tune", tune %>
      </p>
    <% end %>
  </div>

  <%= link_to "New tune", new_tune_path %>
</div>
